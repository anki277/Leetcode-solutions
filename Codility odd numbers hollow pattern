#include<stdio.h>
#include<conio.h>
int main()
{
	int l=1,n;
	printf("Enter the number of rows to print ");
	scanf("%d",&n);
	for(int i=1;i<=n;i++)
	{
		for(int j=1;j<=n;j++)
		{
			if(i==1||i==n||j==1||j==n)
			{ 
			if(l<9)
			{
			  printf("%d",l);
			  l=l+2;
		   }
		   else
		   {
		   	printf("%d",l);
		   	l=1;
		   }
			}
			else
			{
				printf(" ");
			}
		}
		printf("\n");
	}
}
