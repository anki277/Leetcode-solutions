#include<stdio.h>
#include<conio.h>
int main()
{
char ch=65;
int m=1,n=6;
for(int i=0;i<7;i++)
{
printf("%c",ch);
ch++;
}
ch=ch-1;
for(int j=5;j>=0;j--)
{
printf("%c",--ch);
}
printf("\n");
ch=65;
for(int i=0;i<6;i++)
{
	for(int j=0;j<n;j++)
	{
		printf("%c",ch);
		ch++;
	}
	for(int k=0;k<m;k++)
	{
		printf(" ");
	}
	m+=2;
	for(int j=n-1;j>=0;j--)
	{
	printf("%c",--ch);
	}
	printf("\n");
	n--;
}
}
